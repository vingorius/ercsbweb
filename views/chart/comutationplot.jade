extends ../layout/basiclayout

block content
	div.container.chart_base_container
		div.row
			div#comutationplot_sample(style={'float': 'left', 'padding-bottom': '10px', 'margin-left': '130px', 'width': '1140px', 'height': '100px', 'overflow-y': 'hidden', 'overflow-x': 'scroll'})
			div#comutationplot_scale(style={'float': 'left', 'padding': '0px', 'margin': '0px', 'width': '150px', 'height': '100px'})
		div.row
			div#comutationplot_gene(style={'padding': '0px', 'margin-right': '10px', 'float': 'left', 'width': '150px', 'height': '620px'})
			div#comutationplot_mutationseq(style={'padding': '0px', 'margin': '0px', 'float': 'left', 'width': '1100px', 'height': '600px', 'overflow-x' : 'hidden', 'overflow-y': 'hidden', 'border' : '1px solid rgb(236, 236, 236)'})
			div#comutationplot_pq(style={'padding': '0px', 'margin': '0px', 'float': 'left', 'width': '150px', 'height': '620px'})
		div.row
			div#comutationplot_legend(style={'padding': '0px', 'margin': '0px 150px', 'width': '1100px', 'height': '100px'})
