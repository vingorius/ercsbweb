extends ../layout/basiclayout

block content
	div.container.chart_base_container
		div.row
			div#pcaplot_legend(style={'padding': '0px', 'margin': '0px 50px', 'width': '180px', 'height': '66px'})
		div.row
			div#pcaplot_view_3d(style={'padding': '0px', 'margin': '0px', 'width': '1400px', 'height': '850px'})
block prepend scripts
	script(src="/bower_components/threejs/build/three.js")
	script(src="/bower_components/threejs/build/OrbitControls.js")
block append stylesheets
	link(rel='stylesheet', href='/stylesheets/chart/pcaplot.css')		
