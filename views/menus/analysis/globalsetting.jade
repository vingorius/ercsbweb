div#luadModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='luadModalLabel')
	div.modal-dialog(role='document')
		div.modal-content
			div.modal-header
				button.close(type='button', data-dismiss='modal', aria-label='Close')
					span(aria-hidden='true') Ã—
				h4#luadModalLabel.modal-title Filter Background Public Data - LUAD
			div.modal-body
				div#accordion.panel-group(role='tablist', aria-multiselectable='true')
					div.panel.panel-default
						div#heading_a.panel-heading(role='tab')
							h4.panel-title
								a(role='button', data-toggle='collapse', data-parent='#accordion', href='#collapse_a', aria-expanded='true', aria-controls='collapseOne') Expression
								small#sub_message_a.sub_message		
						div#collapse_a.panel-collapse.collapse.in(role='tabpanel', aria-labelledby='heading_a')
							div.panel-body
								div
									input#a_all(type='checkbox',name='a_all', value='All',checked)
									span.sub_menu_span All
								hr(style={'border-top-style': 'dashed','width': '150px', 'margin': '5px'})     
								div
									input(type='checkbox',name='a_item',value='Squamoid')
